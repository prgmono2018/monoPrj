var app = require('../server');
const dotenv = require('dotenv');
dotenv.config();
var port = process.env.PORT;
app.listen(port, function(err) {
    if (err){
 console.log('error !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!' + error);
    }else{
   console.log(`Listening to port ${port}`);
    }
});